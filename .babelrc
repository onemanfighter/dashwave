{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".ts", ".tsx", ".jsx", ".js", ".json"],
        "alias": {
          "components": "./src/components",
          "assets": "./src/assets",
          "provider": "./src/provider",
          "router": "./src/router"
        }
      }
    ]
  ]
}
